{
  "name": "nextjs-starter-npm-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "eslint . --ext .js,.ts,.jsx,.tsx --config .eslintrc.json && next dev -p 1000",
    "build": "eslint . --ext .js,.ts,.jsx,.tsx --config .eslintrc.json && next build",
    "format": "prettier --write \"**/*.js\" \"!node_modules/**/*.js\" && eslint . --fix",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "15.1.0",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.0",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.16"
  }
}
